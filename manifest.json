{
  "manifest_version": 3,
  "name": "Sidebar Extension",
  "version": "1.0",
  "description": "A simple sidebar extension",
  "icons": {
    "16": "icons/icon16.png"
  },
  "permissions": ["sidePanel"],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
} 